<div class="content-container">
    <div class="inventory_content">
        <div id="item-inventory0">
            <div id="inventory_header">
                <h2>INVENTAR - &Uuml;BERSICHT</h2>
                <h3>Inventar anlegen, bearbeiten und l√∂schen</h3>
                <h4>&nbsp;</h4>
            </div>
        </div>
        <div id="item-inventory1">
            <table class="center">           
                <tr>
                    <th>ID</th>
                    <th>Inventar Nr.</th>
                    <th>Bezeichnung</th>
                    <th>Menge</th>
                    <th>Kaufdatum</th>
                    <th>Verf&uuml;gbar</th>
                    <th>Aussortiert am</th>
                    <th>Edit</th>
                    <th>L&ouml;schen</th>
                </tr>
                <% for (var i = 0; i < resultCount; i++) { %> 
                <tr>
                    <td><%=result[i].id%></td>
                    <td><%=result[i].inventory_number%></td>
                    <td><%=result[i].description%></td>
                    <td><%=result[i].amount%></td>
                    <td><%=result[i].purchase_date.getMonth() + "." + result[i].purchase_date.getDay() + "."+result[i].purchase_date.getFullYear()%></td>
                    <td><%=result[i].available%></td>
                    <td><%=result[i].sorted_out_date%></td>
                    <td><a href="/editInventory?id=<%=i%>"><button class="btnedit" type="button">UPDATE</button></a></td>
                    <td><a href="/deleteInventory?id=<%=i%>"><button class="btndelete" type="button">DELETE</button></a></td>

                </tr>
                <%}%>
            </table>
        </div>
        <div id="item-inventory2">
        </div>
    </div>
</div>